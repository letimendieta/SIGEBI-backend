	<queryString language="SQL">
		<![CDATA[select per.cedula,per.nombres, per.apellidos,c.consulta_id,
(select (per.nombres||' '||per.apellidos) as funcionario from funcionarios f
join personas per 
on f.persona_id=per.persona_id 
where f.funcionario_id= c.funcionario_id)
from consultas c
join pacientes p
on  c.paciente_id=p.paciente_id
join personas per
on p.persona_id=per.persona_id 
where c.consulta_id=$P{consultaid}]]>
	</queryString>
