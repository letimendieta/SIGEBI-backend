	<queryString language="SQL">
		<![CDATA[select perPa.cedula, perPa.nombres, perPa.apellidos, c.consulta_id,	
	concat(perFun.nombres,' ' ,perFun.apellidos) as nombreFuncionario,
	a.descripcion as area, fun.reg_prof as registroProf,
	t2.prescripcion_farm as prescripcion 
from consultas c
join pacientes p on  c.paciente_id = p.paciente_id
join personas perPa on p.persona_id = perPa.persona_id 
join funcionarios fun on c.funcionario_id = fun.funcionario_id 
join personas perFun on fun.persona_id = perFun.persona_id 
join areas a on fun.area_id = a.area_id 
left join tratamientos t2 on c.tratamiento_id = t2.tratamiento_id 
where c.consulta_id=$P{consultaid}]]>
	</queryString>
