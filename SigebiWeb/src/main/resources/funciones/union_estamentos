CREATE OR REPLACE FUNCTION public.union_estamentos(mes integer, anho integer)
 RETURNS TABLE(nombreestamento character varying, codcarrera character varying, femenino bigint, masculino bigint, suma numeric)
 LANGUAGE sql
AS $function$
		select * from suma_carrera(mes,anho)
		union all
		select * from suma_departamento(mes,anho)
		union all
		select * from suma_dependencia(mes,anho)
		union all
		select * from suma_postulantes(mes,anho)
		union all 
		select * from suma_estudiantesotrasfacultades(mes,anho)
		union all
		select * from suma_funcionariosotrasfacultades(mes,anho)
		union all
		select * from suma_movilidadestudiantil(mes,anho)
		union all 
		select * from suma_egresados(mes,anho)
		union all 
		select * from suma_cetuna(mes,anho)
		union all 
		select * from suma_familiares(mes,anho)
		union all 
		select * from suma_otros(mes,anho)
$function$
;

