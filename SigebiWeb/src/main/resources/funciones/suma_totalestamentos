CREATE OR REPLACE FUNCTION public.suma_totalestamentos(mes integer, anho integer)
 RETURNS TABLE(femenino bigint, masculino bigint, suma numeric)
 LANGUAGE plpgsql
AS $function$
declare
    f record;
begin
      return query
		SELECT
	       o.femenino, 
	       o.masculino, 
	       SUM(o.femenino + o.masculino) AS Suma
	FROM(
	select count(per.sexo) as femenino,
	(select count(per.sexo) from consultas c
	join pacientes p
	on  c.paciente_id=p.paciente_id
	join personas per
	on p.persona_id=per.persona_id 
	join estamentos es
	on per.estamento_id = es.estamento_id
	where EXTRACT(MONTH FROM fecha)=mes and EXTRACT(YEAR FROM fecha)=anho and per.sexo like '%M%') as masculino
	from consultas c
	join pacientes p
	on  c.paciente_id=p.paciente_id
	join personas per
	on p.persona_id=per.persona_id 
	join estamentos es
	on per.estamento_id = es.estamento_id
	where EXTRACT(MONTH FROM fecha)=mes and EXTRACT(YEAR FROM fecha)=anho and per.sexo like '%F%'
	)
	AS o
	GROUP BY 
		 o.femenino, 
		 o.masculino;
	
END;
$function$
;

